{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "sourceMap": true,
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "downlevelIteration": true,
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "lib": [
            "es6"
        ],
        "outDir": "dist",
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "node_modules"
    ]
}