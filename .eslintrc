{
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "env": {
    "node": true,
    "browser": false
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "max-len": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "no-new": "off",
    "no-unused-expressions": "off",
    "class-methods-use-this": "off",
    "import/extensions": [
      0,
      "ignorePackages"
    ],
    "no-mixed-operators": "off",
    "no-unused-vars": "off",
    "import/prefer-default-export": [
      0,
      "off"
    ],
    "consistent-return": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}
